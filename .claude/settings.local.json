{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(./gradlew:*)",
      "Bash(mv:*)",
      "Bash(rm:*)",
      "Bash(find:*)",
      "Bash(export:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(cp:*)",
      "Bash(cat:*)",
      "Bash(tail:*)",
      "Bash(pwd:*)",
      "Bash(git:log:*)",
      "Bash(git:status:*)",
      "Bash(rg:*)"
    ],
    "deny": [
      "Bash(./gradlew:--stop)",
      "Bash(./gradlew:clean)"
    ]
  },
  "ENV": {
    "BASH_DEFAULT_TIMEOUT_MS": 2400000,
    "BASH_MAX_TIMEOUT_MS": 2400000
  }
}
